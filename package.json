    "react-test-renderer": "^15.4.0",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2"
  },
  "mastarm lint": {
    "ignore": [
      "flow-typed"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "./lib/utils/geojson-snapshot-serializer"
    ]
  }
